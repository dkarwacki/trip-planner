SUPABASE_URL=###
SUPABASE_KEY=###
OPENROUTER_API_KEY=###

# Google Maps API Keys
# Main API key (server-side only, never exposed to client)
# Used for: Geocoding API calls from backend
GOOGLE_MAPS_API_KEY=###

# Optional: Public API key (domain-restricted, for map display)
# Used for: Maps JavaScript API in browser
# If not set, GOOGLE_MAPS_API_KEY will be used (less secure)
GOOGLE_MAPS_API_KEY_PUBLIC=###

# Optional: Map ID (required for AdvancedMarkerElement)
# Create a Map ID in Google Cloud Console: https://console.cloud.google.com/google/maps-apis/studio/maps
# If not set, standard markers will be used instead
GOOGLE_MAPS_MAP_ID=###